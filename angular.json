{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "client_medicanManagementSystem": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/client-medican-management-system",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "src/assets/css/animate.css",
              "src/assets/css/bootstrap-datetimepicker.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap.rtl.min.css",
              "src/assets/css/dataTables.bootstrap5.min.css",
              "src/assets/css/feather.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/plyr.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/js/bootstrap-datetimepicker.min.js",
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/calculator.js",
              "src/assets/js/canada.js",
              "src/assets/js/custom-select2.js",
              "src/assets/js/dataTables.bootstrap5.min.js",
              "src/assets/js/feather.min.js",
              "src/assets/js/forms-pickers.js",
              "src/assets/js/jquery-3.7.1.min.js",
              "src/assets/js/jquery-ui.min.js",
              "src/assets/js/jquery.dataTables.min.js",
              "src/assets/js/jquery.maskedinput.min.js",
              "src/assets/js/jquery.slimscroll.min.js",
              "src/assets/js/jquery.ui.touch-punch.min.js",
              "src/assets/js/jsvectormap.js",
              "src/assets/js/jvectormap.js",
              "src/assets/js/leaflet.js",
              "src/assets/js/mask.js",
              "src/assets/js/modal.js",
              "src/assets/js/moment.min.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/plyr-js.js",
              "src/assets/js/ratings.js",
              "src/assets/js/russia.js",
              "src/assets/js/script.js",
              "src/assets/js/sortable.js",
              "src/assets/js/spain.js",
              "src/assets/js/swiper.js",
              "src/assets/js/theme-colorpicker.js",
              "src/assets/js/theme-script.js",
              "src/assets/js/us-merc-en.js",
              "src/assets/js/validation.js",
              "src/assets/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "client_medicanManagementSystem:build:production"
            },
            "development": {
              "buildTarget": "client_medicanManagementSystem:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
