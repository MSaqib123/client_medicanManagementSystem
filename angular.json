{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "client_medicanManagementSystem": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/client-medican-management-system",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap-datetimepicker.min.css",
              "src/assets/css/animate.css",
              "src/assets/plugins/select2/css/select2.min.css",
              "src/assets/plugins/daterangepicker/daterangepicker.css",
              "src/assets/plugins/tabler-icons/tabler-icons.min.css",
              "src/assets/plugins/fontawesome/css/fontawesome.min.css",
              "src/assets/plugins/fontawesome/css/all.min.css",
              "src/assets/css/bootstrap-datetimepicker.min.css",
              "src/assets/plugins/@simonwep/pickr/themes/nano.min.css",
              "src/assets/plugins/tabler-icons/tabler-icons.min.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.7.1.min.js",
              "src/assets/js/feather.min.js",
              "src/assets/js/jquery.slimscroll.min.js",
              "src/assets/js/jquery.dataTables.min.js",
              "src/assets/js/dataTables.bootstrap5.min.js",
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/plugins/apexchart/apexcharts.min.js",
              "src/assets/plugins/apexchart/chart-data.js",
              "src/assets/plugins/chartjs/chart.min.js",
              "src/assets/plugins/chartjs/chart-data.js",
              "src/assets/js/moment.min.js",
              "src/assets/plugins/daterangepicker/daterangepicker.js",
              "src/assets/plugins/select2/js/select2.min.js",
              "src/assets/plugins/@simonwep/pickr/pickr.es5.min.js",
              "src/assets/js/theme-colorpicker.js",
              "src/assets/js/script.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "client_medicanManagementSystem:build:production"
            },
            "development": {
              "buildTarget": "client_medicanManagementSystem:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
